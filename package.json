{
  "name": "sw-test-env",
  "description": "A sandboxed ServiceWorker environment for testing",
  "version": "2.1.0",
  "author": "Alexander Pope <alex@pope-industries.com>",
  "contributors": [
    "Ola Christian Gundelsby <ola.christian.gundelsby@nrk.no>"
  ],
  "repository": "https://github.com/popeindustries/sw-test-env.git",
  "license": "MIT",
  "keywords": [
    "mock",
    "pseudo",
    "sandbox",
    "service worker",
    "ServiceWorker",
    "test",
    "testing",
    "worker"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rm -f ./test/*.js && rm -rf docs",
    "format": "prettier --write './{src,test}/**/*'",
    "lint": "pnpm run lint:src && pnpm run lint:types",
    "lint:src": "eslint './packages/**/*.js'",
    "lint:types": "tsc --noEmit --skipLibCheck --project jsconfig.json",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "test": "NODE_ENV=test mocha test/*.js --reporter spec --bail"
  },
  "dependencies": {
    "fake-indexeddb": "^3.1.7",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "undici": "^4.16.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chai": "^4.1.2",
    "dvlp": "^14.1.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "mocha": "^5.0.5",
    "prettier": "^2.5.1",
    "typescript": "4.6.2"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{js,json,md,html}": [
      "prettier --write"
    ]
  }
}
